<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="320dp"
    android:layout_height="280dp"
    android:background="@drawable/sdk_main_bg"
    android:layout_gravity="center"
    android:orientation="vertical" >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp" >

        <include layout="@layout/mzyw_pp_title" />

        <include layout="@layout/mzyw_pp_logo" />
    </RelativeLayout>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="40dp"
        android:gravity="center_horizontal"
        >
        <ImageView 
            android:id="@+id/f_newer_qregist"
            android:layout_width="80dp"
            android:layout_height="80dp"
            
            android:src="@drawable/f_newer_qregist"/>
        <ImageView 
            android:id="@+id/f_newer_regist"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/f_newer_regist"/>
        <ImageView 
            android:id="@+id/f_newer_login"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/f_newer_login"/>
    </LinearLayout>
    

</LinearLayout>
